{
  "name": "jdtls",
  "languageId": "java",
  "command": ["jdtls"],
  "settings": {},
  "capabilities": {
    "textDocument": {
      "hover": {
        "contentFormat": ["markdown"]
      }
    }
  },
  "initializationOptions": {
    "settings": {
      "java": {
        "maxConcurrentBuilds": 1,
        "autobuild": {
          "enabled": true
        },
        "import": {
          "maven": {
            "enabled": true
          },
          "gradle": {
            "enabled": true,
            "wrapper": {
              "enabled": true
            }
          },
          "exclusions": [
            "**/node_modules/**",
            "**/.metadata/**",
            "**/archetype-resources/**",
            "**/META-INF/maven/**"
          ]
        },
        "maven": {
          "downloadSources": true
        },
        "configuration": {
          "updateBuildConfiguration": "automatic",
          "checkProjectSettingsExclusions": true,
          "runtimes": [
            {
              "name": "JavaSE-1.8",
              "path": "/Users/xianbin/.jenv/versions/1.8"
            },
            {
              "name": "JavaSE-17",
              "path": "/Users/xianbin/.jenv/versions/17",
              "default": true
            }
          ]
        },
        "project": {
          "importHint": true,
          "importOnFirstTimeStartup": "automatic",
          "referencedLibraries": ["lib/**"],
          "resourceFilters": [
            "node_modules",
            "\\.git",
            ".metadata",
            "archetype-resources",
            "META-INF/maven"
          ]
        },
        "server": {
          "launchMode": "Hybrid"
        },
        "format": {
          "enabled": true
        },
        "contentProvider": {
          "preferred": "procyon cfr fernflower"
        },
        "completion": {
          "guessMethodArguments": true,
          "overwrite": true,
          "enabled": true,
          "lazyResolveTextEdit": {
            "enabled": true
          },
          "favoriteStaticMembers": [
            "org.junit.Assert.*",
            "org.junit.Assume.*",
            "org.junit.jupiter.api.Assertions.*",
            "org.junit.jupiter.api.Assumptions.*",
            "org.junit.jupiter.api.DynamicContainer.*",
            "org.junit.jupiter.api.DynamicTest.*",
            "org.mockito.Mockito.*",
            "org.mockito.ArgumentMatchers.*",
            "org.mockito.Answers.*",
            "org.hamcrest.MatcherAssert.*",
            "org.hamcrest.Matchers.*",
            "org.assertj.core.api.Assertions.*"
          ],
          "filteredTypes": [
            "java.awt.*",
            "com.sun.*",
            "sun.*",
            "jdk.*",
            "io.micrometer.shaded.*"
          ],
          "importOrder": [
            "#",
            "java",
            "javax",
            "jakarta",
            "com",
            "org"
          ]
        },
        "signatureHelp": {
          "enabled": true,
          "description": {
            "enabled": true
          }
        },
        "compile": {
          "nullAnalysis": {
            "mode": "automatic",
            "nonnull": [
              "javax.annotation.Nonnull",
              "org.eclipse.jdt.annotation.NonNull",
              "org.springframework.lang.NonNull",
              "org.jetbrains.annotations.NotNull"
            ],
            "nullable": [
              "javax.annotation.Nullable",
              "org.eclipse.jdt.annotation.Nullable",
              "org.springframework.lang.Nullable",
              "org.jetbrains.annotations.Nullable"
            ]
          }
        }
      }
    },
    "extendedClientCapabilities": {
      "classFileContentsSupport": true,
      "overrideMethodsPromptSupport": true
    },
    "bundles": [
      "/Users/xianbin/.local/jdtls/bundles/jdec.jdt.ls.decompiler.cfr-0.8.0-SNAPSHOT.jar",
      "/Users/xianbin/.local/jdtls/bundles/jdec.jdt.ls.decompiler.common-0.8.0-SNAPSHOT.jar",
      "/Users/xianbin/.local/jdtls/bundles/jdec.jdt.ls.decompiler.fernflower-0.8.0-SNAPSHOT.jar",
      "/Users/xianbin/.local/jdtls/bundles/jdec.jdt.ls.decompiler.procyon-0.8.0-SNAPSHOT.jar",
      "/Users/xianbin/.local/jdtls/bundles/com.microsoft.java.debug.plugin-0.50.0.jar"
    ]
  }
}
